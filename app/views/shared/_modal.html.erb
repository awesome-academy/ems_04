<!-- Modal -->
<div class="modal-dialog" role="document">
  <div class="modal-content">
    <div class="modal-header">
      <h5 class="modal-title" id="exampleModalLabel"><%= yield(:title_modal) %></h5>
      <button type="button" class="close" data-dismiss="modal" aria-label="Close<%= t("app_button.close") %>">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
    <div class="modal-body">
      <%= render yield(:render_form) %>
    </div>
    <div class="modal-footer">
      <button type="button" class="btn btn-secondary" data-dismiss="modal">
        <%= t("app_button.close") %></button>
    </div>
  </div>
</div>
